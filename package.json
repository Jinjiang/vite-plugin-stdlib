{
  "name": "vite-plugin-node",
  "version": "0.0.0",
  "description": "A Vite plugin to polyfill Node.js standard library modules for browser usage.",
  "keywords": [
    "vite",
    "plugin",
    "node",
    "stdlib",
    "browser"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./buffer": {
      "types": "./dist/buffer/index.d.ts",
      "import": "./dist/buffer/index.js",
      "require": "./dist/buffer/index.js"
    },
    "./process": {
      "types": "./dist/process/index.d.ts",
      "import": "./dist/process/index.js",
      "require": "./dist/process/index.js"
    },
    "./global": {
      "types": "./dist/global/index.d.ts",
      "import": "./dist/global/index.js",
      "require": "./dist/global/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "rm -rf node_modules/.vite && vite",
    "demo:serve": "vite",
    "demo:build": "vite build",
    "demo:preview": "vite build && vite preview",
    "build": "tsc -b tsconfig.src.json",
    "watch": "tsc -b --watch tsconfig.src.json",
    "type-check": "tsc --noEmit",
    "prepublish": "pnpm build"
  },
  "peerDependencies": {
    "vite": "8.0.0-beta.11"
  },
  "devDependencies": {
    "@types/node": "^25.2.0",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.3",
    "@vitejs/plugin-vue": "^6.0.4",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "typescript": "^5.9.3",
    "vite": "8.0.0-beta.11",
    "vue": "^3.5.27"
  },
  "dependencies": {
    "@rollup/plugin-inject": "^5.0.5",
    "buffer": "^6.0.3",
    "node-stdlib-browser": "^1.3.1",
    "process": "^0.11.10",
    "vite-plugin-node-polyfills": "^0.25.0"
  }
}
